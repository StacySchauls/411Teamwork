determineState	gol.c	/^int determineState(int row, int col){$/;"	f	language:C
generateInitialGoL	gol.c	/^int generateInitialGoL(){$/;"	f	language:C
grid	gol.h	/^CELL **grid, **nextGrid;$/;"	v	language:C++
